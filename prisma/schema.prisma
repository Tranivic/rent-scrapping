generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Workers {
  id          String  @id @default(uuid())
  dateOut     String  @map("date_out")
  hourOut     String  @map("hour_out")
  dateDev     String  @map("date_dev")
  hourDev     String  @map("hour_dev")
  locationOut String? @map("location_out")
  locationDev String? @map("location_dev")
  intervalMs  Int     @map("interval_ms")

  @@map("workers_queue")
}
